generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Photo {
  id           String   @id @default(uuid())
  title        String
  description  String
  width        Int
  height       Int
  url          String   @unique
  thumbnailUrl String   @unique
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
